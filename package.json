		"eleventy-plugin-images-responsiver": "^2.2.0",
		"luxon": "^3.3.0",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-footnote": "^3.0.3",
		"markdown-it-obsidian-images": "^1.0.1",
		"tailwindcss": "^3.4.3"
